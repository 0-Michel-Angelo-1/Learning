package ud11;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Veiculo v = new Carro();
		
		
		
		while(v.acelerar() >= 0 && v.acelerar() < 10) {
			v.acelerar();
			System.out.println(v.acelerar());
			try { Thread.sleep (500); } catch (InterruptedException ex) {}
		}
		
		
	}

}
