package ud11;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Veiculo v = new Carro();
		
		v.acelerar();
		
		while(v.acelerar()>0) {
			System.out.println(v.acelerar());
			try { Thread.sleep (1000); } catch (InterruptedException ex) {}
		}
		
		
	}

}
